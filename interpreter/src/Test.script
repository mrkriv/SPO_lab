void main()
{
    print((2+4)*10);

    print(pow(2, 10));
    print(pow_rec(2, 10));

    foo2(15 - 1, 2);
}

int pow(int x, int s)
{
    int r = x;
    while(s > 1)
    {
        x *= r;
        s--;
    }

    return x;
}

int pow_rec(int x, int s)
{
    if(s == 1)
    {
        return x;
    }

    return x * pow_rec(x, s - 1);
}

void foo2(int b, int f)
{
    int h = b + 2;
    print(h*f);
}